program Arithmatic_operation
    
    INTEGER :: var1 = 10
    
    INTEGER :: var2 = 20
    
    sum_of_var = var1 + var2 
    
    subtract_of_var = var2 - var1
    
    division_of_var = var2 / var1
    
    multipication_of_var = var1 * var2
    
    modulo_of_var = mod(var1, var2)
    
    power_of_var = (var1 ** var2)
    
    print *, sum_of_var
    
    print *, subtract_of_var
    
    print *, division_of_var
    
    print *, multipication_of_var
    
    print *, modulo_of_var
    
    print *, power_of_var
    
end program Arithmatic_operation